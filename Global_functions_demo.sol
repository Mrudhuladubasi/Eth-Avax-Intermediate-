pragma solidity 0.8.9;

contract globalfunctions_challenge
{
    function getMessageSender() public view returns (address) {
        return msg.sender;
    }

    function getMessageValue() public payable  returns (uint256) {
        return msg.value;
    }

    function getMessageData() public payable  returns (bytes calldata) {
        return msg.data;
    }

}
